{
  "name": "@ksakira10/nwb",
  "description": "A toolkit for React, Preact & Inferno apps, React libraries and other npm modules for the web, with no configuration (until you need it)",
  "version": "0.25.1",
  "license": "MIT",
  "author": "Jonny Buchanan <jonathan.buchanan@gmail.com>",
  "bin": {
    "nwb": "./lib/bin/nwb.js"
  },
  "scripts": {
    "build": "npm run flow && npm run lint && rimraf lib && babel src --out-dir lib --quiet",
    "start": "rimraf lib && babel src --watch --out-dir lib",
    "flow": "flow",
    "lint": "eslint *.js src tests",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run build && npm run test:all",
    "test:coverage": "npm run build && cross-env NODE_ENV=test NWB_TEST=true nyc mocha tests/*-test.js tests/commands/*-test.js",
    "test:all": "cross-env NODE_ENV=test NWB_TEST=true mocha --require babel-register tests/*-test.js tests/commands/*-test.js",
    "test:watch": "cross-env NODE_ENV=test NWB_TEST=true mocha --require babel-register --watch tests/*-test.js"
  },
  "files": [
    "docs",
    "lib",
    "templates",
    "express.js",
    "polyfills.js"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/plugin-transform-react-jsx-self": "^7.8.3",
    "@babel/plugin-transform-react-jsx-source": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@babel/runtime-corejs2": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4",
    "@insin/npm-install-webpack-plugin": "5.0.0",
    "autoprefixer": "9.4.10",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-inferno": "5.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-proposals": "0.1.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "copy-template-dir": "1.4.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-spawn": "6.0.5",
    "css-loader": "2.1.1",
    "debug": "4.1.1",
    "detect-port": "1.3.0",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "figures": "2.0.0",
    "file-loader": "3.0.1",
    "filesize": "4.1.2",
    "fs-extra": "7.0.1",
    "gzip-size": "5.0.0",
    "html-webpack-plugin": "3.2.0",
    "inquirer": "6.2.2",
    "karma": "4.0.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.5",
    "mini-css-extract-plugin": "0.5.0",
    "minimist": "1.2.0",
    "mocha": "^7.1.0",
    "object-assign": "4.1.1",
    "opn": "5.4.0",
    "ora": "3.2.0",
    "phantomjs-prebuilt": "2.1.16",
    "postcss-loader": "3.0.0",
    "promise": "8.0.2",
    "react-hot-loader": "4.3.11",
    "react-hot-loader-loader": "0.0.6",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-noreact": "1.1.0",
    "resolve": "1.10.0",
    "run-series": "1.1.8",
    "semver": "5.6.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "1.1.2",
    "webpack": "4.41.5",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.8.3",
    "@babel/register": "^7.8.6",
    "cross-env": "5.2.0",
    "eslint-config-jonnybuchanan": "6.0.0",
    "eventsource": "1.0.7",
    "flow-bin": "0.82.0",
    "glob": "7.1.3",
    "nyc": "15.0.0",
    "rimraf": "2.6.3",
    "temp": "0.9.0",
    "tree-kill": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KsAkira10/nwb.git"
  },
  "keywords": [
    "development",
    "inferno",
    "preact",
    "react",
    "tooling"
  ],
  "babel": {
    "ignore": [
      "src/react/importModule.js",
      "src/react/renderShim.js"
    ],
    "presets": [
      "@babel/flow",
      [
        "@babel/env",
        {
          "loose": true,
          "targets": {
            "node": "6.11.5"
          }
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "add-module-exports"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "istanbul",
            {
              "include": "src"
            }
          ]
        ]
      }
    }
  },
  "nyc": {
    "instrument": false,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  }
}
